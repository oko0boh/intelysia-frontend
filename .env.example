# Intelysia App Environment Variables
# Copy this file to .env and update the values

# App Configuration
VITE_APP_NAME=Intelysia
VITE_APP_DESCRIPTION="Benin's Premier Business Directory"
VITE_APP_URL=http://localhost:5173

# API Configuration
VITE_API_BASE_URL=http://localhost:5173

# SEO Configuration
VITE_SITE_NAME="Intelysia - Cotonou Business Directory"
VITE_SITE_DESCRIPTION="Discover the best businesses in Cotonou, Benin. Find restaurants, shops, services, and more in your local area."

# Analytics (Optional)
VITE_GA_TRACKING_ID=
VITE_HOTJAR_ID=

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_PWA=true

# Build Configuration
NODE_ENV=development